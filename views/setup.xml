            <field name="name">Terms</field>
            <field name="res_model">condition</field>
<!--            <field name="view_type">form</field>-->
            <field name="view_mode">tree</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Your Terms
                </p>
            </field>
        </record>


    </data>
</odoo>